{
// Enforcing Options
  "bitwise"       : true,
  "browser"       : true,
  "camelcase"     : false,
  "curly"         : true,
  "eqeqeq"        : true,
  "forin"         : true,
  "immed"         : true,
  "indent"        : 2,
  "latedef"       : true,
  "newcap"        : true,
  "noarg"         : true,
  "noempty"       : true,
  "plusplus"      : false,
  "quotmark"      : "single",
  "undef"         : true,
  "unused"        : false,
  "strict"        : false,
  "trailing"      : true,
  "maxlen"        : 120,
  "maxparams"     : 8,
  "maxdepth"      : 5,
  "maxstatements" : 50,

  // Relaxing Options
  "globals": {
    "Canadarm":   true,
    "describe":   false,
    "require":    false,
    "it":         false,
    "beforeEach": false,
    "before":     false,
    "afterEach":  false,
    "after":      false,
    "Image":      false,
    "console":    false,
    "define":     true,
    "module":     true,
    "exports":    true
  }
}